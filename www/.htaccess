DirectoryIndex src/index.php

RewriteEngine On

RewriteRule ^user\/password[\/]?$ src/change_password.php
RewriteRule ^user\/connect[\/]?$ src/index.php
RewriteRule ^user\/disconnect[\/]?$ src/index.php?action=disconnect
RewriteRule ^search[\/]?$ src/search.php

RewriteRule ^dashboard[\/]?$ src/dashboard.view.php